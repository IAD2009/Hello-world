#include <iostream>

int main() {
int gastank = 20, MPGt = 23.5, MPGh = 28.9;
//MPGt = Miles per gallon in a town  
//MPGh = Miles per gallon in a highway
int DistanceT = 20 * 23.5 ;
//Distance T means the distance you would go in a town with one tank of Gas 
int DistanceH = 20 * 28.9 ;
//Distance H means the distance you would go in a highway with one tank of Gas 
std::cout << "Distance for one tank of gas in a town is " << DistanceT << " and the distance for one tank on the highway is " << DistanceH;
}
